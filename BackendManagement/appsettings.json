{
  "DatabaseSettings": {
    "MaxRetryCount": 3,
    "CommandTimeout": 30,
    "MaxBatchSize": 100,
    "EnableSecondLevelCache": false
  },
  "Backup": {
    "Path": "C:\\Backups"
  },
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "DefaultIndex": "backendmanagement"
  },
  "Security": {
    "CorsPolicy": {
      "AllowedOrigins": ["http://localhost:3000"],
      "AllowedMethods": ["GET", "POST", "PUT", "DELETE"],
      "AllowedHeaders": ["*"]
    }
  },
  "Jwt": {
    "Key": "your-secret-key-with-minimum-16-characters",
    "Issuer": "your-issuer",
    "Audience": "your-audience"
  },
  "Logging": {
    "FilePath": "logs"
  }
}
